# Model Configuration
LOCAL_MODEL_DIR=./models/xtts_v2
USE_DEEPSPEED=True
USE_CUDA=True
CACHE_MAX_SIZE=100

# Digital Ocean Spaces Configuration
USE_S3=True
S3_BUCKET=your-spaces-bucket-name
S3_MODEL_PATH=xtts_v2

# Digital Ocean Spaces Authentication
SPACES_KEY=your_spaces_access_key
SPACES_SECRET=your_spaces_secret_key
DO_REGION=nyc3
DO_ENDPOINT_URL=https://nyc3.digitaloceanspaces.com

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000", "https://your-frontend-domain.com"]

# Supabase Authentication
SUPABASE_URL=https://your-supabase-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-role-key